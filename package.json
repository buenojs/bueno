{
  "name": "@buenojs/bueno",
  "version": "0.8.4",
  "description": "A Bun-Native Full-Stack Framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "bueno": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./database": {
      "types": "./dist/database/index.d.ts",
      "import": "./dist/database/index.js"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.js"
    },
    "./migrations": {
      "types": "./dist/migrations/index.d.ts",
      "import": "./dist/migrations/index.js"
    },
    "./orm": {
      "types": "./dist/orm/index.d.ts",
      "import": "./dist/orm/index.js"
    },
    "./jobs": {
      "types": "./dist/jobs/index.d.ts",
      "import": "./dist/jobs/index.js"
    },
    "./notification": {
      "types": "./dist/notification/index.d.ts",
      "import": "./dist/notification/index.js"
    },
    "./template": {
      "types": "./dist/template/index.d.ts",
      "import": "./dist/template/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    }
  },
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "build": "bun build ./src/index.ts ./src/database/index.ts ./src/schema/index.ts ./src/migrations/index.ts ./src/orm/index.ts ./src/jobs/index.ts ./src/templates/index.ts ./src/template/index.ts ./src/notification/index.ts ./src/cli/bin.ts --outdir ./dist --target bun && test -d dist/src && (mv dist/src/* dist/ && rmdir dist/src || true) || true",
    "build:cli": "bun build ./src/cli/bin.ts --outdir ./dist/cli --target bun",
    "typecheck": "tsc --noEmit",
    "lint": "bunx biome check ./src",
    "cli": "bun run src/cli/bin.ts"
  },
  "keywords": [
    "bun",
    "framework",
    "fullstack",
    "http",
    "router",
    "rpc"
  ],
  "author": "Sivaraj D <sivarajd@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@types/bun": "latest",
    "arktype": "^2.0.0",
    "typescript": "^5.3.0",
    "valibot": "^1.0.0",
    "zod": "^3.24.0"
  },
  "peerDependencies": {
    "zod": "",
    "valibot": "",
    "arktype": "",
    "typia": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    },
    "valibot": {
      "optional": true
    },
    "arktype": {
      "optional": true
    },
    "typia": {
      "optional": true
    }
  },
  "engines": {
    "bun": ">=1.3.0"
  }
}
